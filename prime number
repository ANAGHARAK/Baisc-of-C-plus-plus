#include <iostream>
using namespace std;
int main() {
    // Write C++ code here
   int a;
   cin>>a;
   int flag=0;
   for(int i=2;i<=a/2;i++){
       if(a%i==0){
           flag=1;
           break;
       }
   }
   if(flag==1){
       cout<<"The number "<<a<<" is not a prime number"<<endl;
   }
   else{
       cout<<"The number "<<a<<" is a prime number"<<endl;
   }
    return 0;
}
